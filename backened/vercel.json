{
  "version": 2,
  "name": "Full Stack",
  "builds": [
    {
      "src": "ecom/backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ecom/backend/index.js",
      "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
        "Access-Control-Allow-Origin":"*"
      }
    }
  ]
}
